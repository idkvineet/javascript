// this is a temperature converter
const kelvin=293;
// getting celsius value
let celsius=kelvin-273;
//converting to faherneit
let faherneit=celsius*(9/5)+32;
//getting intger valur
faherneit=Math.floor(faherneit);
console.log(`The temperature is ${faherneit} degrees Fahrenheit.`);
let newton=celsius*(33/100);
newton=Math.floor(newton);
console.log(`The temperature is ${newton} degrees Newton.`);
